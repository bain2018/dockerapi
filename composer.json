{
    "name": "bain2018/dockerapi",
    "license": "MIT",
    "type": "library",
    "description": "A Docker Api Client",
    "autoload": {
        "psr-4": {
            "Docker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docker\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "bain2018/dockerphpapi": "^5.0.0",
        "guzzlehttp/psr7": "dev-master",
        "php-http/client-common": "^2.5.0",
        "php-http/socket-client": "^2.1.0",
        "php-http/message": "^1.0",
        "symfony/filesystem": "^2.3 || ^3.0 || ^4.0",
        "symfony/process": "^2.3 || ^3.0 || ^4.0"
    },
    "suggest": {
        "php-http/httplug-bundle": "For integration with Symfony",
        "amphp/artax": "To use the async api"
    },
    "require-dev": {
        "amphp/artax": "^3.0",
        "amphp/socket": "^0.10.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
